<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<parent>
		<groupId>com.github.navikt</groupId>
		<artifactId>tjenestespesifikasjoner-tbd</artifactId>
		<version>0-SNAPSHOT</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<artifactId>nav-virksomhet-oppdragsbehandling-v1-meldingsdefinisjon</artifactId>
	<packaging>jar</packaging>
	<version>0-SNAPSHOT</version>

	<properties>
		<xsd.location>${project.basedir}/src/main/xsd/no/trygdeetaten/skjema/oppdrag</xsd.location>
	</properties>
	<dependencies>
		<dependency>
			<groupId>com.migesok</groupId>
			<artifactId>jaxb-java-time-adapters</artifactId>
			<version>1.1.3</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>jaxb2-maven-plugin</artifactId>
				<version>1.5</version>
				<executions>
					<execution>
						<id>xjc</id>
						<goals>
							<goal>xjc</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<extension>true</extension>
					<arguments>-Xvalue-constructor -Xfluent-api</arguments>
					<bindingDirectory>${project.basedir}/src/main/xsd/</bindingDirectory>
					<schemaDirectory>${xsd.location}</schemaDirectory>
				</configuration>
				<dependencies>
					<dependency>
						<groupId>org.jvnet.jaxb2_commons</groupId>
						<artifactId>jaxb2-value-constructor</artifactId>
						<version>3.0</version>
					</dependency>
					<dependency>
						<groupId>net.java.dev.jaxb2-commons</groupId>
						<artifactId>jaxb-fluent-api</artifactId>
						<version>2.1.8</version>
					</dependency>
				</dependencies>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<executions>
					<execution>
						<id>attach-sources</id>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

	<scm>
		<developerConnection>scm:git:git@github.com:navikt/tjenestespesifikasjoner-tbd.git</developerConnection>
		<connection>scm:git:git@github.com:navikt/tjenestespesifikasjoner-tbd.git</connection>
		<url>https://github.com/navikt/tjenestespesifikasjoner-tbd</url>
		<tag>HEAD</tag>
	</scm>
	<name>${project.artifactId}</name>
</project>
